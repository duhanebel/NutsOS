{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "attach",
            "name": "Attach to QEMU",
            "executable": "${workspaceFolder}/build/kernelfull.o",
            "gdbpath": "/usr/bin/gdb-multiarch",
            "target": "localhost:1234",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "start QEMU",
            "postDebugTask": "stop QEMU",
            "valuesFormatting": "parseText",
            "autorun": [
                "set confirm off",
                "Set pagination off",
                "add-symbol-file ${workspaceFolder}/build/kernelfull.o 0x100000",
                //"target remote | qemu-system-i386 -hda ./bin/os.bin -S -gdb stdio"
            ]
        },
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "enter program name, for example ${workspaceFolder}/a.out",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb-multiarch",
            "stopAtEntry": true,
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            //"debugServerPath": "/usr/bin/qemu-system-i386",
            "project": "build/kernelfull.o",
            "program": "/usr/bin/qemu-system-i386",
            "args": [
                "-hda ./bin/os.bin",
                "-s",
                "-S",
                "-nographic"
            ],
            //"debugServerArgs": "-hda ./bin/os.bin -s -S -nographic",
            // "args": [
            //     "-ex",
            //     "set confirm off",
            //     "-ex",
            //     "set pagination off",
            //     "-ex",
            //     "add-symbol-file build/kernelfull.o 0x100000",
            //     //  "-ex",
            //     //  "target remote localhost:1234"
            // ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "logging": {
                "programOutput": true
            },
            "miDebuggerPath": "/usr/bin/gdb-multiarch",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "text": "-target-select remote localhost:1234"
                },
                // {
                //     "text": "-ex \"set confirm off\""
                // },
                // {
                //     "text": "-ex \"set pagination off\""
                // },
                // {
                //     "text": "-ex \"add-symbol-file build/kernelfull.o 0x100000\""
                // },
                // { "text": "target remote localhost:1234"},
            ]
        }
    ]
}